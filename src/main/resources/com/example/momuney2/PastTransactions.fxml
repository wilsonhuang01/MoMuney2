<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="600.0" prefWidth="700.0" stylesheets="@StylesheetPT.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.momuney2.controllers.PastTransactionsController">
   <children>
      <VBox layoutX="25.0" layoutY="75.0" prefHeight="72.0" prefWidth="650.0">
         <children>
            <HBox prefHeight="30.0" prefWidth="650.0">
               <children>
                  <TextField fx:id="searchName" prefHeight="30.0" prefWidth="200.0" promptText="Name">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </TextField>
                  <DatePicker fx:id="searchDate" prefHeight="30.0" prefWidth="145.0" promptText="Date">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin></DatePicker>
                  <ChoiceBox fx:id="searchCategory" prefHeight="30.0" prefWidth="145.0">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </ChoiceBox>
                  <ChoiceBox fx:id="sortBy" prefHeight="30.0" prefWidth="145.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </HBox>
            <Line endX="648.0" stroke="#e0e0e0" strokeWidth="2.0" />
         </children>
      </VBox>
      <HBox fx:id="navBar" prefHeight="50.0" prefWidth="700.0">
         <children>
            <Label prefHeight="44.0" prefWidth="200.0" text="MoMuney" textFill="WHITE">
               <padding>
                  <Insets left="20.0" />
               </padding>
               <HBox.margin>
                  <Insets right="130.0" />
               </HBox.margin>
               <font>
                  <Font name="Caveat" size="28.0" />
               </font>
            </Label>
            <Button alignment="CENTER" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="120.0" text="New" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Damascus Regular" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Button>
            <StackPane prefWidth="120.0">
               <children>
                  <Rectangle arcHeight="30.0" arcWidth="30.0" fill="#0c6183" height="40.0" stroke="#ffffff00" strokeType="INSIDE" width="120.0">
                     <StackPane.margin>
                        <Insets />
                     </StackPane.margin></Rectangle>
                  <Button fx:id="pastTransactionsButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="120.0" text="Transactions" textFill="WHITE">
                     <font>
                        <Font name="Damascus Regular" size="15.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </StackPane>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="120.0" text="Summary" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Damascus Regular" size="15.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <ScrollPane layoutX="25.0" layoutY="117.0" prefHeight="460.0" prefWidth="650.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="460.0" prefWidth="650.0" />
        </content>
      </ScrollPane>
   </children>
</AnchorPane>
